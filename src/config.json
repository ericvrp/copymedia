{
	"requireProjectName": true,
	"progressReport": true,
	"simulate": false,
	"thumbnail": {
		"maxParallelTasks": 100000,
		"task": "nconvert/nconvert.exe -out jpeg -embedded_jpeg -o \"DEST\" \"SRC\""
	},
	"allMedia": [
		{
			"extensions": [".ARW"],
			"set_xmp": ["stars", "color", "accept_reject"],
			"set_exif": ["internet_rating"],
			"source": "E:/",
			"destination": {
				"path": "C:/Users/eric/Nextcloud/ericvrp2.stackstorage.com/PhotosAndVideos",
				"postfix": "/photos"
			} 
		},
		{
			"extensions": [".MP4"],
			"source": "E:/",
			"destination": {
				"path": "C:/Users/eric/Nextcloud/ericvrp2.stackstorage.com/PhotosAndVideos",
				"postfix": "/videos"
			} 
		}
	]
}
