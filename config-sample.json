{
	"requireProjectName": true,
	"progressReport": true,
	"simulate": false,
	"looping": {
		"enabled": false,
		"intervalInSeconds": 5
	},
	"allMedia": [
		{
			"extensions": [".ARW"],
			"set_xmp": ["stars", "color", "accept_reject"],
			"set_exif": ["internet_rating"],
			"source": "E:/",
			"destination": {
				"path": "D:/ericvrp2.stackstorage.com/PhotosAndVideos",
				"postfix": "/photos"
			} 
		},
		{
			"extensions": [".MP4"],
			"source": "E:/",
			"destination": {
				"path": "D:/ericvrp2.stackstorage.com/PhotosAndVideos",
				"postfix": "/videos"
			} 
		}
	]
}
